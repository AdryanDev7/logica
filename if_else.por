programa {
  funcao inicio() {
    
    cadeia login
    cadeia senha

    escreva("Preencha seu nome: ")
    leia(login)

    escreva("Preencha com sua senha: ")
    leia(senha)

    se(login == "Adryan" e senha == "123"){
      escreva("Login feito com o sucesso!")
    }senao{
      escreva("Nome de usuario incorreto")
    }
  
  }
}
